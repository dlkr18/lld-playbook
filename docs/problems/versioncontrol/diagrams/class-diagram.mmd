classDiagram
    class Repository {
        -String repoId
        -String name
    }
    class Commit {
        -String commitId
        -String message
    }
    class Branch {
        -String branchId
        -String name
    }
    class VersionControlService {
        <<interface>>
        +commit()
        +createBranch()
    }
    VersionControlService --> Repository
    Repository --> Commit
    Repository --> Branch
