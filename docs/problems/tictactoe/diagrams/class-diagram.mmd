classDiagram
    class Board {
        -Cell[][] cells
        -int size
        +makeMove()
        +checkWinner()
    }
    class Player {
        -String name
        -Symbol symbol
    }
    class Game {
        -Board board
        -Player[] players
        +start()
        +playTurn()
    }
    Game --> Board
    Game --> Player
    Board --> Player
