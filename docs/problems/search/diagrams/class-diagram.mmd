classDiagram
    class SearchEngine {
        -InvertedIndex index
        +indexDocument()
        +search()
    }
    class InvertedIndex {
        -Map~String,List~Document~~ index
        +add()
        +search()
    }
    class Document {
        -String docId
        -String content
        -Map~String,int~ termFrequency
    }
    class SearchResult {
        -Document doc
        -double score
    }
    SearchEngine --> InvertedIndex
    InvertedIndex --> Document
    SearchEngine --> SearchResult
