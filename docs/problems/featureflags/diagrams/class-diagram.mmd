classDiagram
    class FeatureFlag {
        -String flagId
        -boolean enabled
        -Map~String,Variant~ variants
    }
    class Variant {
        -String name
        -int percentage
    }
    class FeatureFlagService {
        -Map~String,FeatureFlag~ flags
        +isEnabled()
        +getVariant()
    }
    FeatureFlagService --> FeatureFlag
    FeatureFlag --> Variant
