classDiagram
    class Stock {
        -String symbol
        -double currentPrice
    }
    class Order {
        -String orderId
        -OrderType type
        -double price
    }
    class Trade {
        -String tradeId
        -double price
    }
    class StockExchangeService {
        <<interface>>
        +placeOrder()
        +matchOrders()
    }
    StockExchangeService --> Stock
    StockExchangeService --> Order
    StockExchangeService --> Trade
